{
    "name": "smewebify/greensustainability",
    "type": "project",
    "description": "GreenSustainAbility is an advanced online solution for environmental management.",
    "keywords": [
        "laravel",
        "Environmental management",
        "WEB"
    ],
    "authors": [
        {
            "name": "k√©vin niglaut"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "composer-runtime-api": "^2.2",
        "brick/math": "^0.9.3|^0.10.2|^0.11",
        "doctrine/inflector": "^2.0.5",
        "dragonmantank/cron-expression": "^3.3.2",
        "egulias/email-validator": "^3.2.1|^4.0",
        "fruitcake/laravel-cors": "dev-develop",
        "fruitcake/php-cors": "^1.2",
        "guzzlehttp/uri-template": "^1.0",
        "laravel/framework": "^10.0",
        "laravel/prompts": "^0.1.9",
        "laravel/serializable-closure": "^1.3",
        "league/commonmark": "^2.2.1",
        "league/flysystem": "^3.8.0",
        "livewire/livewire": "3.5.2",
        "monolog/monolog": "^3.0",
        "nesbot/carbon": "^2.67",
        "nunomaduro/termwind": "^1.13",
        "psr/container": "^1.1.1|^2.0.1",
        "psr/log": "^1.0|^2.0|^3.0",
        "psr/simple-cache": "^1.0|^2.0|^3.0",
        "ramsey/uuid": "^4.7",
        "spatie/laravel-activitylog": "^4.7",
        "tijsverkoyen/css-to-inline-styles": "^2.2.5",
        "vlucas/phpdotenv": "^5.4.1",
        "voku/portable-ascii": "^2.0",
        "willvincent/feeds": "^2.3"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.17",
        "laravel/sail": "^1.12",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
